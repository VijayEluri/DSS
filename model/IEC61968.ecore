        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="statusDateTime" eType="ecore:EDataType iec61970.ecore#//domain/AbsoluteDateTime">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Date and time for which 'status' applies."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="statusRemarks" eType="ecore:EDataType iec61970.ecore#//domain/String">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Information about the current 'status'."/>
        </eAnnotations>
