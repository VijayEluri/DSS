          <details key="documentation" value="The status at the time of 'statusDateTime'.  Refer to instances of associated ActivityRecords for prior states."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="statusDateTime" eType="ecore:EDataType iec61970.ecore#//domain/AbsoluteDateTime">
