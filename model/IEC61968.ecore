        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="GmlGraphics" upperBound="-1"
          eType="#//gml_support/GmlGraphic" eOpposite="#//gml_support/GmlGraphic/GmlMarks"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="GmlStrokes" upperBound="-1"
          eType="#//gml_support/GmlStroke" eOpposite="#//gml_support/GmlStroke/GmlMarks"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="GmlFIlls" upperBound="-1"
