        <details key="documentation" value="Defines a &quot;shape&quot; which has coloring applied to it (i.e. square, circle, triangle, star, ...)."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="wellKnownName" eType="ecore:EDataType iec61970.ecore#//domain/String">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
