        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="GmlFeatureStyles" lowerBound="1"
          upperBound="-1" eType="#//gml_support/GmlFeatureStyle"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="GmlBaseSymbol" eType="#//gml_support/GmlBaseSymbol"
          eOpposite="#//gml_support/GmlBaseSymbol/GmlSymbols"/>
