        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="POS" eType="ecore:EDataType iec61970.ecore#//domain/Float">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="A number designating the position of a point in N-dimensional space"/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="FeatureCollection" lowerBound="1"
          upperBound="-1" eType="#//gml_support/FEATURECOLLECTION" eOpposite="#//gml_support/FEATURECOLLECTION/Envelopes"/>
