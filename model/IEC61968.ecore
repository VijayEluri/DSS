    <eClassifiers xsi:type="ecore:EClass" name="PricingStructure" eSuperTypes="#//documentation/documentinheritance/Document">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="A grouping of pricing components and prices used in the creation of customer charges and the eligibility criteria under which these terms may be offered to a customer.  The reasons for grouping includes state, customer classification, site characteristics, classification (i.e. fee price structure, deposit price structure, electric service price structure, etc.) and accounting requirements. "/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="code" eType="ecore:EDataType iec61970.ecore#//domain/String">
