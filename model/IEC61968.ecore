      <eStructuralFeatures xsi:type="ecore:EAttribute" name="dailyEstUsage" eType="ecore:EDataType iec61970.ecore#//domain/Integer">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Daily Estimate Usage Quantity is a manually entered usage quantity typically entered by customer accounting.  It is used in place of actual computed estimated average when history of usage is not available. "/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="dailyCeilingUsage" eType="ecore:EDataType iec61970.ecore#//domain/Integer">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Daily Ceiling Usage Quantity is the absolute maximum valid non-demand usage quantity used in validating a customer's billed non-demand usage.  "/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="dailyFloorUsage" eType="ecore:EDataType iec61970.ecore#//domain/Integer">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Daily Floor Usage Quantity is the absolute minimum valid non-demand usage quantity used in validating a customer's billed non-demand usage. "/>
        </eAnnotations>
