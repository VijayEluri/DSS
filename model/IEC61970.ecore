          eOpposite="#//Topology/ConnectivityNode/Terminals">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Terminals interconnect with zero impedance at a node.  Measurements on a node apply to all of its terminals."/>
        </eAnnotations>
      </eStructuralFeatures>
