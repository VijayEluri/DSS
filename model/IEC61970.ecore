          <details key="documentation" value="ConductingEquipment has 1 or 2 terminals that may be connected to other ConductingEquipment terminals via ConnectivityNodes"/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="RegulatingControl" upperBound="-1"
          eType="#//Wires/RegulatingControl" eOpposite="#//Wires/RegulatingControl/Terminal"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="Measurements" upperBound="-1"
