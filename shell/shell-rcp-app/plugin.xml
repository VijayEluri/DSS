<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="com.ncond.dss.shell.rcp.DSSApplication">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="shell-rcp-app.application"
            name="DSS">
         <property
               name="appName"
               value="DSS">
         </property>
      </product>
   </extension>

</plugin>
